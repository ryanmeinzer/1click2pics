{
  "expo": {
    "name": "1click2pics",
    "slug": "1click2pics",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ryanmeinzer.1click2pics",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Give 1click2pics permission to access your photos so that you may choose which photos to display on your screen. Your original photos will never be deleted and will only be copied into a new 1click2pics album on your phone for your privacy and safety."
      }
    },
    "android": {
      "package": "com.ryanmeinzer.oneclicktwopics",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}